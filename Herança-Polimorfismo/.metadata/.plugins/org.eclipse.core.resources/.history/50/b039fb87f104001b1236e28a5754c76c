
public class TesteAnimal {
	public static void main(String args[]) {
		Animal Cachorro = new Animal("Dora", 3, "AuAuAu");
		Cavalo Cavalo = new Cavalo("Dorivalda", 13, " hinn in in", "corre");
		Animal Preguiça = new Animal("Cachorro", 10, "ióó ió");
		
		System.out.println(Cachorro.getNome());
		System.out.println(Cavalo.getEmiteSom());
		System.out.println(Preguiça.getIdade());
	}
	
}
