
public class Aviao {
	private int dia;
	private String mes;
	private int ano;
	private int numeroVoo;
	
	public Aviao(int dia, String mes, int ano, int numeroVoo) {
		dia = dia;
		mes = mes;
		ano = ano;
		numeroVoo = numeroVoo;
	}
	
	public class getDadosVoo() {
		String dadosDoVoo = dia + "/" + mes + "/" + ano + " - " + numeroVoo; 
	}
	
}
